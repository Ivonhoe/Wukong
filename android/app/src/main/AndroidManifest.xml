<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hellobike.magiccube">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher2"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:name=".demo.MyApp"
        android:theme="@style/Theme.Exmaple"
        android:testOnly="false"
        tools:replace="android:allowBackup"
        tools:targetApi="n">
        <activity android:name=".demo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".demo.MagicListActivity" />

        <activity android:name=".demo.preview.PreviewActivity" />

        <activity android:name=".demo.preview.PreviewListActivity" />

    </application>
</manifest>