<?xml version="1.0" encoding="UTF-8" ?>
<template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.w3school.com.cn"
    xsi:schemaLocation="http://www.w3school.com.cn magic.xsd" lifecycle="false">
 <container background="#dedede" padding-vertical="20rpx" width="100%" display="flex" flex-direction="column" justify-content="center" align-items="center">
  <!-- icon -->
  <img src="https://appstoreisvpic.alipayobjects.com/prod/d031e1ce-37f1-427e-b606-0d195e39e6a3.png" width="200rpx" height="200rpx" />
  <!-- message -->
  <span margin-top="10rpx" font-size="32rpx" content="${message}" color="#000000">
   <keyword content="${keyword}" color="#ff0000" font-weight="bold"/>
  </span>
 </container>
</template>